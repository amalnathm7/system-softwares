PG1	START	1000
	LDX	ZERO
	STX	INDEX1
	STX	INDEX2
LOOP	LDA	DATA1,X
	AND	MAX
	COMP	MAX
	JEQ	L1
	J	L2
L1	LDX	INDEX1
	LDA	DATA1,X
	LDX	INDEX2
	STA	DATA2,X
	LDA	INDEX2
	ADD	THREE
	STA	INDEX2
L2	LDA	INDEX1
	ADD	THREE
	STA	INDEX1
	LDX	INDEX1
	COMP	LENGTH
	JLT	LOOP
	RSUB
ZERO	WORD	0
THREE	WORD	3
LENGTH	WORD	15
MAX	WORD	8388608
DATA1	RESW	5
INDEX1	RESW	5
DATA2	RESW	1
INDEX2	RESW	1
	END
